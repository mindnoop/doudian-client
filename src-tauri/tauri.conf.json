{
  "build": {
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "doudian-client",
    "version": "1.0.5"
  },
  "tauri": {
    "allowlist": {
      "all": true,
      "http": {
        "all": true,
        "request": true
      }
    },
    "bundle": {
      "active": true,
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "com.tauri.doudian",
      "targets": "all"
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": true,
      "dialog": true,
      "endpoints": ["https://raw.githubusercontent.com/mindnoop/achievement/main/doudian.json"],
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDIyQURBMEEzQTA1MTU1N0UKUldSK1ZWR2dvNkN0SWkyRElyakZSOEdrRjAxRk5DQ21RTkdFKzJCb2JHMlhjbngrblBZR0Z6TEMK"
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "doudian-client",
        "width": 800,
        "height": 600
      }
    ]
  }
}
